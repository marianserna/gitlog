{
  "name": "cotd",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "concurrently": "3.0.0",
    "react-scripts": "0.6.1"
  },
  "dependencies": {
    "history": "4.2.0",
    "re-base": "2.2.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "4.0.0-alpha.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names 'webpack, sass' --prefix name 'npm run start' 'npm run styles:watch'",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "sass ./src/css/style.scss ./src/css/style.css",
    "styles:watch": "sass --watch ./src/css/style.scss:./src/css/style.css"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
